<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    >

    <TextView
        android:id="@+id/mTetle1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:textSize="20dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mMz1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"

        app:layout_constraintBottom_toBottomOf="@+id/mZd1"
        app:layout_constraintStart_toEndOf="@+id/mZd1"
        app:layout_constraintTop_toTopOf="@+id/mZd1" />

    <TextView
        android:id="@+id/mGs1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"

        app:layout_constraintStart_toEndOf="@+id/mMz1"
        app:layout_constraintTop_toTopOf="@+id/mMz1" />

    <TextView
        android:id="@+id/mSj1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"

        app:layout_constraintStart_toEndOf="@+id/mGs1"
        app:layout_constraintTop_toTopOf="@+id/mGs1" />

    <ImageView
        android:id="@+id/mZd1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/mTetle1"
        app:layout_constraintTop_toBottomOf="@+id/mTetle1"
        app:srcCompat="@drawable/zd" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:background="@color/colorHs"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/mTetle1"
        app:layout_constraintStart_toStartOf="@+id/mTetle1" />

</android.support.constraint.ConstraintLayout>
